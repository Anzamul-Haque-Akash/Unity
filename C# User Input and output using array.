using System;
class HelloWorld {
  static void Main() {
    
    Console.Write("Enter the size of an array : ");

    int n = Convert.ToInt32(Console.ReadLine());

    string[] names = new string[n];
    
    for (int i=0; i<n; i++){
        Console.Write("Enter the Name : ");
        names[i] = Console.ReadLine();
    }
    
    for (int i=0; i<n; i++){
        Console.WriteLine(names[i]);
    }
    
  }
}
